//unions
#include <iostream>
#include <sstream>
#include <string>
using namespace std;


int main ()
{
union mine {
	char c;
	int i;
	float f;
	} mytypes;

	mytypes.i=10;
	cout << "ready for output\n";
	cout << "char c = "  << mytypes.c; 
	cout << "int i = " << mytypes.i;
	cout << "float f = " << mytypes.f;
	return 0;
}

